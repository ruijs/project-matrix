{
  "private": true,
  "name": "project-matrix",
  "version": "0.0.3",
  "description": "Project management system based on Rui & Rapid.",
  "main": "lib/index.js",
  "scripts": {
    "build": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon -r tsconfig-paths/register ./index.ts --watch ./index.ts",
    "dev:remix": "remix watch",
    "prod": "remix build && run-p start",
    "start": "cross-env NODE_ENV=production ts-node-esm --project ./tsconfig-node.json -r tsconfig-paths/register ./index.ts",
    "gen-meta": "ts-node-esm --project ./tsconfig-node.json -r dotenv/config -r tsconfig-paths/register ./app/tools/gen-meta.ts",
    "update-rpd-models": "ts-node-esm --project ./tsconfig-node.json -r dotenv/config -r tsconfig-paths/register ./app/tools/update-rpd-models.ts",
    "test": "echo \"Error: no test specified\"",
    "gen:svg-icons": "npx @svgr/cli --out-dir ./app/shopfloor-extension/rocks/custom-icons/components -- ./app/shopfloor-extension/rocks/custom-icons/svgs",
    "fmt:code": "prettier . --write"
  },
